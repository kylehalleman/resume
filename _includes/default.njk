<body class="layout">
  <header>
  <nav aria-label="Top level navigation menu">
    <ul role="list" class="site-nav">
      {%- for entry in collections.all | eleventyNavigation %}
      <li class="site-nav__item">
        <a href="{{ entry.url }}" class="text-trim"{% if entry.url == page.url %} aria-current="page"{% endif %}>{{ entry.title }}</a>
      </li>
      {%- endfor %}
    </ul>
  </nav>
  </header>
  <main>
    {{ content | safe }}
  </main>
  <footer>
    <ul role="list" class="social">
      <li class="social__item">
        <a href="https://github.com/kylehalleman/resume">View the source</a>
      </li>
      <li class="social__item">
        <a href="https://www.linkedin.com/in/kylehalleman/">LinkedIn</a>
      </li>
      <li class="social__item">
        <a href="mailto:jobs@kylehalleman.com">Email</a>
      </li>
    </ul>
  </footer>
</body>

</html>